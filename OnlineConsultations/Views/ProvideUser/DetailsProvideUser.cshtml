@model DetailsProvideUserModelView

@{
    ViewBag.Title = $"ProvideUser - {Model.ProvideUserId}";
}


<h2 class="text-center">@ViewBag.Title</h2>
<hr />

<div class="text-center row">
    <div class="card col-4" style="width: 20rem; ">
        <div class="card-body">
            <input type="hidden" asp-for="@Model.ProvideUserId" />
            <h5 class="card-title mt-1">ProvideUser ID: @Model.ProvideUserId</h5>
            <p class="mb-0">Application User Id: @Model.ApplicationUserId</p>
        </div>
    </div>
</div>

