@model IEnumerable<AllGuestUsersModelView> 


@{
    ViewBag.Title = "All Guest Users";
}


<h2 class="text-center">@ViewBag.Title</h2>
<hr />

<a class="btn btn-warning" asp-controller="GuestUser" asp-action="AllGuestUsers"
   sp-route-information="" class="btn btn-success">All GuestUsers</a>
<hr />

<div class="text-center row">
    @if (Model.Count()  > 0)
    {
        @foreach (var guestUser in Model)
        {
            <div class="card col-4" style="width: 20rem;">
                <div class="card-body">
                    <h5 class="card-title mt-1">GuestUser ID: @guestUser.GuestUserId</h5>
                    <h5 class="card-title mt-1">GuestUser ID: @guestUser.ApplicationUserId</h5>
                </div>               
            </div>
        }
    }
    else 
    {
        <p class="mb-0">There are no guest users yet!</p>
    }
 
</div>