@model DeleteSearchUserModelView

@{
    ViewBag.Title = "Delete SearchUser";
}

<h2 class="text-center">@ViewBag.Title</h2>
<hr />

<form asp-action="Delete">
    <div class="row">
        <div class="col-md-8">
            <input type="hidden" asp-for="@Model.SearchUserId" />
            <div class="form-group">
                <label asp-for="SearchUserId" class="control-label">Search User Id</label>
                <input asp-for="SearchUserId" class="form-control" disabled="disabled" />
            </div>
            <div class="form-group">
                <label asp-for="ApplicationUserId" class="control-label">Application User Id</label>
                <input asp-for="ApplicationUserId" class="form-control" disabled="disabled" />
            </div>
        </div>
    </div>
</form>